#include <Arduino.h>

// Deklarieren diverser Variablen
int FanInput = 0;
int TestAInput = 0;
int FanPWM = 9;             // Fan PWM pin 

void setup() {
  Serial.begin(9600);  
  Serial.println("--- Start Serial Monitor SEND_RCVE ---");

  pinMode(FanPWM, OUTPUT);

}

void loop() {

  // AInput zu zweitem String
  FanInput = analogRead(TestAInput);

  Serial.print("AInput: ");
  Serial.print("\t");
  Serial.print(FanInput);
  Serial.println();

  analogWrite(FanPWM, FanInput);

}